from mqtt://localhost/#influxerdev {
     watch "orobridge/p1/tele/tea/SENSOR" jsonp {
           measurement "tea"
           "total"      <- "/ENERGY/Total"
           "yesterday"  <- "/ENERGY/Yesterday"
           "today"      <- "/ENERGY/Today"
           "power"      <- "/ENERGY/Power"
           "voltage"    <- "/ENERGY/Voltage"
           "current"    <- "/ENERGY/Current"
     }
     watch "sj/#"  auto
}
